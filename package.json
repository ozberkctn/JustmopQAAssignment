{
  "name": "Justmop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.1.5",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "metro-react-native-babel-preset": "^0.49.2",
    "moment": "^2.22.2",
    "react": "^16.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-native": "git+https://github.com/ozberkctn/react-native-blurOnSubmitVersion.git",
    "react-native-calendars": "git+https://github.com/ozberkctn/react-native-calendars.git",
    "react-native-dev-menu": "^2.0.0",
    "react-native-i18n": "^2.0.15",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-keyboard-aware-scroll-view": "^0.7.4",
    "react-native-layout-tester": "^1.0.6",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-masked-text": "^1.9.2",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-splash-screen": "^3.1.1",
    "react-navigation": "^2.18.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.0.0-beta6",
    "i": "^0.3.6",
    "jest": "^23.4.1",
    "npm": "^6.4.1",
    "prop-types": "^15.6.2",
    "react-dom": "^16.6.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true,
    "preset": "react-native",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ios.js",
      "native.js",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(|react-native|react-native-loading-spinner-overlay|react-navigation|react-native-safe-area-view|react-navigation-deprecated-tab-navigator|react-native-calendars|react-native-iphone-x-helper|react-native-i18n)/)"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
